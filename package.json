{
  "name": "yarsk",
  "version": "1.0.0",
  "description": "My React starter kit. Webpack, Babel, Sass, React 0.13 and more.",
  "main": "app/index.js",
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
    "test": "./node_modules/karma/bin/karma start conf/karma.conf.js --single-run",
    "test:watch": "./node_modules/karma/bin/karma start conf/karma.conf.js",
    "test:coverage": "./node_modules/karma/bin/karma start conf/karma.coverage.conf.js --single-run",
    "test:ci": "./node_modules/karma/bin/karma start conf/karma.ci.conf.js --single-run",
    "dev": "./node_modules/.bin/webpack-dev-server --config conf/webpack.config.js --hot --progress --colors --inline --content-base ./build",
    "build": "./node_modules/.bin/webpack --config conf/webpack.production.js",
    "build:gh": "npm run build && node ./build-gh-pages.js && rm -r ./dist",
    "start": "babel-node ./server/index.js",
    "postinstall": "npm run build"
  },
  "author": "Brad Daily <brad@bradleyboy.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer-loader": "^1.2.0",
    "babel": "^4.7.16",
    "babel-core": "^4.7.8",
    "babel-eslint": "^2.0.2",
    "babel-loader": "^4.1.0",
    "classnames": "^1.2.0",
    "compression": "^1.4.3",
    "css-loader": "^0.9.1",
    "dotenv": "^1.1.0",
    "eslint": "^0.17.1",
    "eslint-loader": "^0.7.0",
    "express": "^4.12.3",
    "extract-text-webpack-plugin": "^0.3.8",
    "file-loader": "^0.8.1",
    "html-webpack-plugin": "^1.1.0",
    "immutable": "^3.7.1",
    "json-loader": "^0.5.1",
    "less": "^2.4.0",
    "less-loader": "^2.1.0",
    "react": "~0.13.0",
    "sass-loader": "^0.4.2",
    "style-loader": "^0.8.3",
    "twitter": "^1.2.5",
    "twitter-text": "^1.11.0",
    "url-loader": "^0.5.5",
    "webpack": "^1.7.3",
    "ws": "^0.7.1"
  },
  "devDependencies": {
    "gh-pages": "^0.2.0",
    "isparta": "^2.2.0",
    "isparta-instrumenter-loader": "^0.1.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.2.7",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^1.0.2",
    "karma-notify-reporter": "^0.1.1",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-webpack": "^1.5.0",
    "react-hot-loader": "^1.2.3",
    "webpack-dev-server": "^1.7.0"
  }
}
